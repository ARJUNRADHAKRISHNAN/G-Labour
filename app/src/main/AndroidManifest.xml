<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phoneotp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AppliancesServices"></activity>
        <activity android:name=".HomeMaintanence" />
        <activity
            android:name=".EmployeeReviewActivity"
            android:label="@string/title_activity_employee_review"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CustomerReviewActivity"
            android:label="@string/title_activity_customer_review"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ServiceCategoryActivity"
            android:label="@string/title_activity_service_category"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SessionActivity"
            android:label="@string/title_activity_session"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".OtpActivity" />
    </application>

</manifest>